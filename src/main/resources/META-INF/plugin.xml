    <name>Dotnet Warehouse</name>
    <vendor>rafaelldi</vendor>

    <depends>com.intellij.modules.platform</depends>

    <resource-bundle>messages.DotnetWarehouseBundle</resource-bundle>

    <extensions defaultExtensionNs="com.intellij">
